#User function Template for python3
class Solution:
    def isPossible(self, S):
        l=len(S)
        if (l%2==0):
            if S[0:l//2]==S[l//2:]:
                return 1
            else:
                return 0
        else:
            if S[0:l//2]==S[(l//2)+1:]:
                return 1
            else:
                return 0
        # code here
